   "source": [
    "def power_function(base, power):\n",
    "    if power == 1:\n",
    "        return base\n",
    "    \n",
    "    else:\n",
    "        p = base * power_function(base, power - 1)\n",
    "        return p\n",
    "    \n",
    "power_function(3, 5)"
   ]
